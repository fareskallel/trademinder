# Example configuration for TraderMind backend

# Gateway → Orchestrator
ORCHESTRATOR_HOST=127.0.0.1
ORCHESTRATOR_PORT=8001

# Orchestrator → Feedback Service
FEEDBACK_SERVICE_HOST=127.0.0.1
FEEDBACK_SERVICE_PORT=8003

# Feedback Service → LLM Service
LLM_HOST=127.0.0.1
LLM_PORT=8002

# LLM model used by llm_service
LLM_MODEL=llama3.2

# Ollama instance
OLLAMA_HOST=http://localhost:11434
